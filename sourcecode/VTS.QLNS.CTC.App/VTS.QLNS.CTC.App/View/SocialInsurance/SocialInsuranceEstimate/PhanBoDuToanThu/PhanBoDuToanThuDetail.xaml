<Window x:Class="VTS.QLNS.CTC.App.View.SocialInsurance.SocialInsuranceEstimate.PhanBoDuToanThu.PhanBoDuToanThuDetail"
        x:Name="PhanBoDuToanThuDetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:localEnum="clr-namespace:VTS.QLNS.CTC.Utility.Enum;assembly=VTS.QLNS.CTC.Utility"
        xmlns:extensions="clr-namespace:VTS.QLNS.CTC.App.Extensions"
        xmlns:compt="clr-namespace:VTS.QLNS.CTC.App.Component"
        xmlns:helper="clr-namespace:VTS.QLNS.CTC.App.Helper"
        xmlns:ext="clr-namespace:VTS.QLNS.CTC.App.Extensions"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        Style="{StaticResource MaterialDesignWindow}" 
        Title="Quản lý thu chi bảo hiểm - Phân bổ DT thu BHXH, BHYT, BHTN"
        Height="768" Width="1400">
    <Window.InputBindings>
        <!--<KeyBinding Key="F3" Command="{Binding AddCommand}" />
        <KeyBinding Key="F4" Command="{Binding DeleteCommand}" CommandParameter="{Binding ElementName=dgPhanBoDuToan, Path=.}"/>-->
        <KeyBinding Key="F10" Command="{Binding SaveDataCommand}" />
        <KeyBinding Key="F5" Command="{Binding RefreshCommand}" />
    </Window.InputBindings>
    <materialDesign:DialogHost
        Identifier="PhanBoDuToanThuDetaillDialog"
        DialogTheme="Inherit">
        <Grid Margin="16">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <StackPanel 
                Grid.Row="0"
                Orientation="Vertical">
                <DockPanel>
                    <materialDesign:PackIcon Kind="Dollar" Style="{StaticResource DialogScreenIconStyle}"/>
                    <StackPanel Orientation="Vertical" VerticalAlignment="Center">
                        <TextBlock Text="CHI TIẾT ĐỢT PHÂN BỔ" FontWeight="Bold"/>
                        <TextBlock>
                            <TextBlock.Text>
                                <MultiBinding StringFormat="Năm làm việc: {0} | Số: {1} - Số QĐ: {2} - Ngày QĐ: {3}">
                                    <Binding Path="NamLamViec" />
                                    <Binding Path="Model.SSoChungTu" />
                                    <Binding Path="Model.SSoQuyetDinh" />
                                    <Binding Path="Model.DNgayQuyetDinh" Converter="{StaticResource DateTimeToStringConverter}" />
                                </MultiBinding>
                            </TextBlock.Text>
                        </TextBlock>
                    </StackPanel>
                </DockPanel>
                <Separator Background="{DynamicResource MaterialDesignDivider}"/>
            </StackPanel>
            <StackPanel Grid.Row="1">
                <StackPanel Orientation="Horizontal">
                    <StackPanel Orientation="Horizontal" Visibility="{Binding Model.BKhoa, Converter={StaticResource InvertBooleanToVisibilityConverter}}">
                        <StackPanel Orientation="Horizontal" Visibility="{Binding IsAdjusted, Converter={StaticResource BooleanToVisibilityConverter}}">
                            <Button 
                                extensions:Permission.Identifier="BUDGET_ESTIMATE_DIVISION_DETAIL_BTN_ADD"
                                Style="{StaticResource SearchBarBtnStyle}"
                                Command="{Binding AddCommand}">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="Plus" />
                                    <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Tạo mới (F3)"/>
                                </StackPanel>
                            </Button>
                            <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                        </StackPanel>
                        <Button 
                            Style="{StaticResource SearchBarBtnStyle}" 
                            IsEnabled="{Binding IsSaveData}"
                            Command="{Binding SaveDataCommand}">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="ContentSave" />
                                <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Lưu dữ liệu (F10)"/>
                            </StackPanel>
                        </Button>
                        <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                    </StackPanel>
                    <Button 
                        Style="{StaticResource SearchBarBtnStyle}"
                        ToolTip="Refresh"
                        Command="{Binding RefreshCommand}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="Refresh" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Tải lại (F5)"/>
                        </StackPanel>
                    </Button>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}"
                               Visibility="{Binding IsShowPlanButton, Converter={StaticResource BooleanToVisibilityConverter}}" />
                    <Button
                            Visibility="{Binding IsShowPlanButton, Converter={StaticResource BooleanToVisibilityConverter}}"
                            Style="{StaticResource SearchBarBtnStyle}" 
                        Command="{Binding GetPlanDataCommand}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="ApplicationImport" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Margin="5 0 5 0" Text="Lấy dữ liệu kế hoạch"/>
                        </StackPanel>
                    </Button>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}"
                               Visibility="{Binding IsShowAggregateAdjustButtonPBDTT, Converter={StaticResource BooleanToVisibilityConverter}}" />
                    <Button
                            Visibility="{Binding IsShowAggregateAdjustButtonPBDTT, Converter={StaticResource BooleanToVisibilityConverter}}"
                            Style="{StaticResource SearchBarBtnStyle}" 
                        Command="{Binding GetAggregateAdjustDataCommand}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="ApplicationImport" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Margin="5 0 5 0" Text="Lấy dữ liệu điều chỉnh tổng hợp"/>
                        </StackPanel>
                    </Button>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                    <Button
                        x:Name="btnPrint"
                        IsEnabled="False"
                        extensions:Permission.Identifier="BUDGET_ESTIMATE_DIVISION_DETAIL_BTN_PRINT_REPORT"
                        Style="{StaticResource SearchBarBtnStyle}" 
                        Command="{Binding BtnPrintCommand}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="Printer" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}">In báo cáo</TextBlock>
                            <materialDesign:PopupBox
                                Margin="-10 0 0 0"
                                Grid.Column="1" 
                                HorizontalAlignment="Right"
                                IsPopupOpen="{Binding IsOpenPrintPopup, Mode=TwoWay}">
                                <materialDesign:PopupBox.ToggleContent>
                                    <materialDesign:PackIcon Kind="MenuDown" />
                                </materialDesign:PopupBox.ToggleContent>
                                <StackPanel>
                                    <Button
                                        Command="{Binding PrintActionCommand}"
                                        CommandParameter="{x:Static localEnum:SocialInsuranceDivisionEstimatePrintType.APPENDIX}"
                                        Content="Thông báo giao dự toán thu BHXH, BHYT, BHTN" />
                                    <Button
				                        Command="{Binding PrintActionCommand}"
				                        CommandParameter="{x:Static localEnum:SocialInsuranceDivisionEstimatePrintType.COVER_SHEET}"
                                        Content="Thông báo giao dự toán - Tổng hợp thu, chi" />
                                    <Button
                                        Command="{Binding PrintActionCommand}"
                                        CommandParameter="{x:Static localEnum:SocialInsuranceDivisionEstimatePrintType.TARGET_AGENCY}"
                                        Content="Thông báo điều chỉnh, bổ sung dự toán - Tổng hợp thu chi" />
                                    <Button 
                                        Command="{Binding PrintActionCommand}"
                                        CommandParameter="{x:Static localEnum:SocialInsuranceDivisionEstimatePrintType.DU_TOAN_THU_CHI_TONG_HOP}"
                                        Content="Tổng hợp dự toán thu, chi BHXH, BHYT, BHTN" />
                                    <Button Command="{Binding PrintActionCommand}"
                                            CommandParameter="{x:Static localEnum:SocialInsuranceDivisionEstimatePrintType.DELIVER}"
                                            Content="Phương án giao dự toán thu BHXH, BHYT, BHTN" />
                                </StackPanel>
                            </materialDesign:PopupBox>
                        </StackPanel>
                    </Button>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                    <Button x:Name="btnShowCol" Style="{StaticResource SearchBarBtnStyle}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="PlusMinusVariant" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Ẩn/hiện cột" />
                            <materialDesign:PopupBox
                                Name="pbDynamicColumn"
                                Margin="-10 0 0 0"
                                HorizontalAlignment="Right"
                                StaysOpen="True">
                                <materialDesign:PopupBox.ToggleContent>
                                    <materialDesign:PackIcon Kind="MenuDown"/>
                                </materialDesign:PopupBox.ToggleContent>
                            </materialDesign:PopupBox>
                        </StackPanel>
                    </Button>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                    <ComboBox
                        Name="cbFrozenColumn"
                        materialDesign:HintAssist.Hint="Cố định cột"
                        DisplayMemberPath="DisplayItem"
                        SelectedValuePath="ValueItem"/>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                    <Button 
                        Style="{StaticResource SearchBarBtnStyle}" 
                        Command="{Binding CloseCommand}"
                        CommandParameter="{Binding ElementName=PhanBoDuToanThuDetailWindow}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="CloseOutline" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Đóng"/>
                        </StackPanel>
                    </Button>
                </StackPanel>
                <Separator Background="{DynamicResource MaterialDesignDivider}"/>
                <StackPanel Orientation="Horizontal">
                    <TextBox
                        Margin="0 0 -20 0"
                        materialDesign:TextFieldAssist.HasClearButton="True"
                        materialDesign:HintAssist.Hint="Mô tả"
                        Style="{StaticResource SearchBarHorizontalTextBoxStyle}"
                        Text="{Binding SNoiDungSearch, UpdateSourceTrigger=PropertyChanged }"
                        Width="{Binding ElementName=Description, Path=ActualWidth}">
                        <TextBox.InputBindings>
                            <KeyBinding Command="{Binding SearchCommand}" Key="Return" />
                        </TextBox.InputBindings>
                    </TextBox>
                    <materialDesign:PopupBox
                        Margin="-13 0 0 0"
                        Grid.Column="1"
                        HorizontalAlignment="Right"
                        StaysOpen="True"
                        IsPopupOpen="{Binding IsPopupOpen, Mode=TwoWay}">
                        <materialDesign:PopupBox.ToggleContent>
                            <materialDesign:PackIcon Kind="MenuDown" />
                        </materialDesign:PopupBox.ToggleContent>
                        <Grid Margin="0 10 -10 0" Height="400">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <TextBox
                                Grid.Row="0"
                                materialDesign:HintAssist.Hint="Tìm kiếm"
                                Text="{Binding PopupSearchText, UpdateSourceTrigger=PropertyChanged}" />
                            <DataGrid
                                Grid.Row="1"
                                Margin="0 10 0 0"
                                Name="dgdPopup"
                                ItemsSource="{Binding DataPopupSearchItems}"
                                AutoGenerateColumns="False"
                                CanUserResizeColumns="True"
                                BorderThickness="1"
                                SelectedItem="{Binding SelectedPopupItem}"
                                CanUserAddRows="False"
                                RowStyle="{StaticResource DataGridRowDetail}">
                                <DataGrid.Columns>
                                    <DataGridTextColumn
                                        Header="Mô tả"
                                        Binding="{Binding SMoTa}"
                                        IsReadOnly="True" />
                                </DataGrid.Columns>
                            </DataGrid>
                        </Grid>
                    </materialDesign:PopupBox>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                    <ComboBox Width="{Binding ElementName=idDonVi, Path=ActualWidth}"
                        materialDesign:HintAssist.Hint="-- Đơn vị --"
                        materialDesign:TextFieldAssist.HasClearButton="True"
                        DisplayMemberPath="DisplayItem"
                        Margin="-5 0 0 0"
                        SelectedItem="{Binding SelectedAgency}"
                        ItemsSource="{Binding Agencies}"
                        Style="{StaticResource SearchBarHorizontalComboBoxStyle}" />
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" Margin="20 0 0 0"/>
                    <ComboBox
                        Margin="-20 0 0 0"
                        materialDesign:HintAssist.Hint="Có dữ liệu"
                        DisplayMemberPath="DisplayItem"
                        SelectedValuePath="ValueItem"
                        SelectedValue="{Binding TypeDisplaysSelected}"
                        ItemsSource="{Binding TypeDisplays}"
                        Style="{StaticResource SearchBarHorizontalComboBoxStyle}" >
                    </ComboBox>
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" Visibility="Collapsed"/>
                    <ComboBox
                        Visibility="Collapsed"
                        materialDesign:HintAssist.Hint="-- Nhóm ngành --"
                        materialDesign:TextFieldAssist.HasClearButton="True"
                        DisplayMemberPath="DisplayMemberPath"
                        SelectedItem="{Binding SelectedNNganh}"
                        ItemsSource="{Binding NNganhItems}"
                        Style="{StaticResource SearchBarHorizontalComboBoxStyle}" />
                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" Visibility="Collapsed"/>
                    <ComboBox
                        Visibility="Collapsed"
                        materialDesign:HintAssist.Hint="-- Chuyên ngành --"
                        materialDesign:TextFieldAssist.HasClearButton="True"
                        DisplayMemberPath="DisplayMemberPath"
                        SelectedItem="{Binding SelectedCNganh}"
                        ItemsSource="{Binding CNganhItems}"
                        Style="{StaticResource SearchBarHorizontalComboBoxStyle}" />

                    <Separator Visibility="{Binding ShowColNSSD}" Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                    <Button
                            Style="{StaticResource SearchBarBtnStyle}"
                            IsEnabled="{Binding IsEnablePhanBoAll}"
                            Visibility="Hidden"
                            Command="{Binding PhanBoConLaiCommand}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="Transfer" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Phân bổ toàn bộ cho đơn vị đang chọn" />
                        </StackPanel>
                    </Button>
                </StackPanel>
            </StackPanel>
            <Grid Grid.Row="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <compt:ExpandedDataGrid
                        x:Name="dgPhanBoDuToan"
                        Grid.Row="2"
                        Margin="0 3 0 0"
                        VerticalAlignment="Top"
                        BorderThickness="1"
                        AutoGenerateColumns="False"
                        CanUserResizeColumns="True"
                        HorizontalScrollBarVisibility="Auto"
                        FrozenColumnCount="6"
                        CanUserAddRows="False"
                        CanUserDeleteRows="False"
                        GridLinesVisibility="All"
                        ElementFrozenColumn="{Binding ElementName=cbFrozenColumn, Path=.}"
                        ElementDynamicColumn="{Binding ElementName=pbDynamicColumn, Path=.}"
                        IsSaveDataGridInfo="False"
                        Style="{StaticResource MaterialDesignDataGrid}"
                        CellStyle="{StaticResource DataGridCellDetail}"
                        RowStyle="{StaticResource DataGridRowEstimateDetail}"
                        ItemsSource="{Binding Items}"
                        ext:DataGridBehavior.LastColumnFill="True"
                        SelectedItem="{Binding SelectedItem}"
                        ScrollViewer.ScrollChanged="dgdData_ScrollChanged">
                        <DataGrid.Resources>
                            <helper:BindingProxy x:Key="DataContextProxy" Data="{Binding}" />
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <!--<DataGridTextColumn x:Name="LNS" Header="LNS" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding SLns}" IsReadOnly="True"/>
                            <DataGridTextColumn x:Name="L" Header="L" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding SL}" IsReadOnly="True"/>
                            <DataGridTextColumn x:Name="K" Header="K" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding SK}" IsReadOnly="True"/>
                            <DataGridTextColumn x:Name="M" Header="M" Visibility="{Binding Data.ColumnDisplay.ColumnM, Source={StaticResource DataContextProxy}}" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding SM}" IsReadOnly="True"/>
                            <DataGridTextColumn x:Name="TM" Header="TM" Visibility="{Binding Data.ColumnDisplay.ColumnTM, Source={StaticResource DataContextProxy}}" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding STm}" IsReadOnly="True"/>
                            <DataGridTextColumn x:Name="TTM" Header="TTM" Visibility="{Binding Data.ColumnDisplay.ColumnTTM, Source={StaticResource DataContextProxy}}" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding STtm}" IsReadOnly="True"/>
                            <DataGridTextColumn x:Name="NG" Header="NG" Visibility="{Binding Data.ColumnDisplay.ColumnNG, Source={StaticResource DataContextProxy}}" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding SNg}" IsReadOnly="True"/>
                            <DataGridTextColumn x:Name="TNG" Header="TNG" Visibility="{Binding Data.ColumnDisplay.ColumnTNG, Source={StaticResource DataContextProxy}}" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding STng}" IsReadOnly="True"/>-->
                            <DataGridTextColumn x:Name="Description" Header="Mô tả" ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" Binding="{Binding SMoTa}" IsReadOnly="True"/>
                            <DataGridTemplateColumn Header="Đơn vị" x:Name="idDonVi">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <DataTemplate.Resources>
                                            <Style TargetType="ComboBox" BasedOn="{StaticResource GridComboBoxStyle}"></Style>
                                            <Style TargetType="TextBlock" BasedOn="{StaticResource DataGridTextColumnDetailStyle}"></Style>
                                        </DataTemplate.Resources>
                                        <StackPanel>
                                            <ComboBox
                                                Visibility="{Binding BEmpty, Converter={StaticResource BooleanToVisibilityConverter}}"
                                                SelectedValuePath="ValueItem"
                                                ItemsSource="{Binding CbxDonVi, Mode=TwoWay}"
                                                SelectedValue="{Binding IIdMaDonVi, Mode=TwoWay}">
                                                <ComboBox.ItemTemplate>
                                                    <DataTemplate>
                                                        <DataTemplate.Resources>
                                                            <Style TargetType="TextBlock" BasedOn="{StaticResource DataGridTextColumnDetailStyle}"></Style>
                                                        </DataTemplate.Resources>
                                                        <StackPanel>
                                                            <TextBlock Text="{Binding DisplayItem}"></TextBlock>
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </ComboBox.ItemTemplate>
                                                <ComboBox.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <VirtualizingStackPanel />
                                                    </ItemsPanelTemplate>
                                                </ComboBox.ItemsPanel>
                                            </ComboBox>
                                            <TextBlock 
                                                Visibility="{Binding BEmpty, Converter={StaticResource InvertBooleanToVisibilityConverter}}"
                                                Text="{Binding STenDonVi}">
                                            </TextBlock>
                                        </StackPanel>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <!--Trước điều chỉnh-->
                            <compt:DataGridTextColumn
                            x:Name="NLDDongBHXHTruocDieuChinh"
                            Header="NLĐ đóng trước điều chỉnh (BHXH)"
                            StringFormat="{}{0:N0}"
                            IsReadOnly="True"
                            Binding="{Binding FBHXHNLDTruocDieuChinh, Converter={ StaticResource NumberToStringEmptyConverter}, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                            Visibility="{Binding Data.ColumnVisibility.IsDisplayBhxhDieuChinh, Converter={StaticResource BooleanToVisibilityConverter}, Source={StaticResource DataContextProxy}}"
                            EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}"
                            HeaderStyle="{StaticResource DataGridColumnHeaderNoBorder}">
                                    <DataGridColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Border
                                            Padding="{Binding RelativeSource={RelativeSource Self}, Path=(materialDesign:DataGridAssist.ColumnHeaderPadding), Converter={StaticResource HeaderPaddingConverter}}">
                                                    <Canvas Height="{Binding ElementName=HeaderGroup, Path=ActualHeight}">
                                                        <Grid>
                                                            <Grid.Style>
                                                                <Style TargetType="Grid">
                                                                    <Setter Property="Width">
                                                                        <Setter.Value>
                                                                            <MultiBinding Converter="{StaticResource MultipleWidthConverter}">
                                                                            <Binding ElementName="NLDDongBHXHTruocDieuChinh" Path="ActualWidth"/>
                                                                            <Binding ElementName="NSDDongBHXHTruocDieuChinh" Path="ActualWidth"/>
                                                                            <Binding ElementName="CongBHXHTruocDieuChinh" Path="ActualWidth"/>
                                                                            </MultiBinding>
                                                                        </Setter.Value>
                                                                    </Setter>
                                                                </Style>
                                                            </Grid.Style>
                                                            <TextBlock Name="HeaderGroup" HorizontalAlignment="Center" Text= "Thu BHXH trước điều chỉnh" />
                                                        </Grid>
                                                    </Canvas>
                                                </Border>
                                                <Border
                                        Grid.Row="1"
                                        Padding="{Binding RelativeSource={RelativeSource Self}, Path=(materialDesign:DataGridAssist.ColumnHeaderPadding)}"
                                        BorderThickness="0 1 1 0"
                                        BorderBrush="{Binding HorizontalGridLinesBrush, RelativeSource={RelativeSource AncestorType=DataGrid}}">
                                                    <TextBlock Text="NLĐ đóng" HorizontalAlignment="Center" />
                                                </Border>
                                            </Grid>
                                        </DataTemplate>
                                    </DataGridColumn.HeaderTemplate>
                                </compt:DataGridTextColumn>
                                <compt:DataGridTextColumn
                            x:Name="NSDDongBHXHTruocDieuChinh"
                            Header="NSD đóng trước điều chỉnh (BHXH)"
                            IsReadOnly="True"
                            StringFormat="{}{0:N0}"
                            Binding="{Binding FBHXHNSDTruocDieuChinh, Converter={ StaticResource NumberToStringEmptyConverter}, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                            Visibility="{Binding Data.ColumnVisibility.IsDisplayBhxhDieuChinh, Converter={StaticResource BooleanToVisibilityConverter}, Source={StaticResource DataContextProxy}}"
                            EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}"
                            HeaderStyle="{StaticResource DataGridColumnHeaderNoBorder}">
                                    <DataGridColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Border
                                            Grid.Row="1"
                                            Padding="{Binding RelativeSource={RelativeSource Self}, Path=(materialDesign:DataGridAssist.ColumnHeaderPadding)}"
                                            BorderThickness="0 1 1 0"
                                            BorderBrush="{Binding HorizontalGridLinesBrush, RelativeSource={RelativeSource AncestorType=DataGrid}}">
                                                    <TextBlock Name="HeaderGroup" HorizontalAlignment="Center" Text="NSD đóng"/>
                                                </Border>
                                            </Grid>
                                        </DataTemplate>
                                    </DataGridColumn.HeaderTemplate>
                                </compt:DataGridTextColumn>
                                <compt:DataGridTextColumn
                            x:Name="CongBHXHTruocDieuChinh"
                            Header="Cộng trước điều chỉnh (BHXH)"
                            StringFormat="{}{0:N0}"
                            Binding="{Binding FThuBHXHTruocDieuChinh, Converter={ StaticResource NumberToStringEmptyConverter}, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                            Visibility="{Binding Data.ColumnVisibility.IsDisplayBhxhDieuChinh, Converter={StaticResource BooleanToVisibilityConverter}, Source={StaticResource DataContextProxy}}"
                            IsReadOnly="True"
                            EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}"
                            HeaderStyle="{StaticResource DataGridColumnHeaderNoBorder}">
                                    <DataGridColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Border
                                            BorderThickness="0 0 1 0"
                                            BorderBrush="{Binding HorizontalGridLinesBrush, RelativeSource={RelativeSource AncestorType=DataGrid}}"
                                            Padding="{Binding RelativeSource={RelativeSource Self}, Path=(materialDesign:DataGridAssist.ColumnHeaderPadding), Converter={StaticResource HeaderPaddingConverter}}">
                                                    <Canvas Height="{Binding ElementName=HeaderGroup, Path=ActualHeight}">
                                                    </Canvas>
                                                </Border>
                                                <Border
                                            Grid.Row="1"
                                            Padding="{Binding RelativeSource={RelativeSource Self}, Path=(materialDesign:DataGridAssist.ColumnHeaderPadding)}"
                                            BorderThickness="0 1 1 0"
                                            BorderBrush="{Binding HorizontalGridLinesBrush, RelativeSource={RelativeSource AncestorType=DataGrid}}">
                                                    <TextBlock Name="HeaderGroup" HorizontalAlignment="Center" Text="Cộng"/>
                                                </Border>
                                            </Grid>
                                        </DataTemplate>
                                    </DataGridColumn.HeaderTemplate>
                                </compt:DataGridTextColumn>
                                <compt:DataGridTextColumn
                            x:Name="NLDDongBHYTTruocDieuChinh"
                            Header="NLĐ đóng trước điều chỉnh (BHYT)"
                            StringFormat="{}{0:N0}"
                            IsReadOnly="True"
                            Binding="{Binding FBHYTNLDTruocDieuChinh, Converter={ StaticResource NumberToStringEmptyConverter}, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                            Visibility="{Binding Data.ColumnVisibility.IsDisplayBhxhDieuChinh, Converter={StaticResource BooleanToVisibilityConverter}, Source={StaticResource DataContextProxy}}"
                            EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}"
                            HeaderStyle="{StaticResource DataGridColumnHeaderNoBorder}">
                                    <DataGridColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Border
                                            Padding="{Binding RelativeSource={RelativeSource Self}, Path=(materialDesign:DataGridAssist.ColumnHeaderPadding), Converter={StaticResource HeaderPaddingConverter}}">
                                                    <Canvas Height="{Binding ElementName=HeaderGroup, Path=ActualHeight}">
                                                        <Grid>
                                                            <Grid.Style>
                                                                <Style TargetType="Grid">
                                                                    <Setter Property="Width">
                                                                        <Setter.Value>
                                                                            <MultiBinding Converter="{StaticResource MultipleWidthConverter}">
                                                                            <Binding ElementName="NLDDongBHYTTruocDieuChinh" Path="ActualWidth"/>
                                                                            <Binding ElementName="NSDDongBHYTTruocDieuChinh" Path="ActualWidth"/>
                                                                            <Binding ElementName="CongBHYTTruocDieuChinh" Path="ActualWidth"/>
                                                                            </MultiBinding>
                                                                        </Setter.Value>
                                                                    </Setter>
                                                                </Style>
                                                            </Grid.Style>
                                                            <TextBlock Name="HeaderGroup" HorizontalAlignment="Center" Text= "Thu BHYT trước điều chỉnh" />
                                                        </Grid>
                                                    </Canvas>
                                                </Border>
                                                <Border
                                        Grid.Row="1"
