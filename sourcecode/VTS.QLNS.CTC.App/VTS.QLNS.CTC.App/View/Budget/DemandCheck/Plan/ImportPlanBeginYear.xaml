<Window x:Class="VTS.QLNS.CTC.App.View.Budget.DemandCheck.Plan.ImportPlanBeginYear"
             Name="PlanBeginYearImportWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VTS.QLNS.CTC.App.View.Budget.DemandCheck.Plan"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:wpf="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:helper="clr-namespace:VTS.QLNS.CTC.App.Helper"
             xmlns:component="clr-namespace:VTS.QLNS.CTC.App.Component"
             mc:Ignorable="d" 
             WindowStartupLocation="CenterScreen"
            WindowState="Maximized">
    <Window.InputBindings>
        <KeyBinding Key="F6" Command="{Binding OpenReferencePopupCommand}" CommandParameter="{Binding ElementName=DgMLNS, Path=.}" />
    </Window.InputBindings>
    <Grid Margin="16">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <DockPanel Grid.Row="0">
                <materialDesign:PackIcon Kind="MicrosoftExcel" Style="{StaticResource DialogScreenIconStyle}"/>
                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Margin="10 0">
                    <TextBlock Text="IMPORT DỮ LIỆU DỰ TOÁN ĐẦU NĂM" FontWeight="Bold"/>
                    <TextBlock Text="Chọn file Excel"/>
                </StackPanel>
            </DockPanel>
            <Separator Grid.Row="1" Background="{DynamicResource MaterialDesignDivider}"/>
            <Grid Grid.Row="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Border  BorderThickness="1" BorderBrush="{DynamicResource MaterialDesignDivider}" Margin="0 10 0 0" Padding="5 0">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBox
                        Grid.Column="0"
                        IsEnabled="False"
                        materialDesign:HintAssist.Hint="Lựa chọn file excel"
                        materialDesign:TextFieldAssist.DecorationVisibility="Hidden"
                        BorderThickness="0"
                        VerticalAlignment="Center" 
                        Text="{Binding FilePath}"/>
                        <Button
                        Style="{DynamicResource MaterialDesignToolButton}"
                        Grid.Column="1"
                        Command="{Binding UploadFileCommand}">
                            <materialDesign:PackIcon
                            Kind="FileUpload"
                            Margin="8 0 0 0" />
                        </Button>
                    </Grid>
                </Border>

                <Grid Grid.Row="1"
                Margin="0 10 0 0">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBox
                            VerticalAlignment="Center"
                            materialDesign:HintAssist.IsFloating="True"
                            Grid.Column="0"
                            materialDesign:HintAssist.Hint="Số chứng từ"
                            IsReadOnly="True"
                            Text="{Binding SoChungTu, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" />
                        <DatePicker
                            Grid.Column="1"
                            Language="vi-VN"
                            Margin="10 12 0 0"
                            TabIndex="0"
                            materialDesign:HintAssist.Hint="Ngày chứng từ"
                            Text="{Binding NgayChungTu, Mode=TwoWay}" />
                        <ComboBox 
                            Name="CbChungTu"
                            Grid.Column="2"
                            Margin="10 0 0 0"
                            SelectionChanged="CbChungTu_SelectionChange"
                            materialDesign:HintAssist.Hint="Chọn chứng từ"
                            Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                            DisplayMemberPath="DisplayItem"
                            SelectedValuePath="ValueItem"  
                            SelectedItem="{Binding SelectedChungTu, UpdateSourceTrigger=PropertyChanged}"
                            ItemsSource="{Binding DataChungTu}"/>
                        <ComboBox 
                            Grid.Column="3"
                            Margin="10 0 0 0"
                            materialDesign:HintAssist.Hint="Chọn đơn vị"
                            Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                            DisplayMemberPath="DisplayItem"
                            SelectedValuePath="ValueItem"  
                            SelectedItem="{Binding SelectedDonVi}"
                            ItemsSource="{Binding DataDonVi}"/>
                        <ComboBox 
                            Grid.Column="4"
                            Margin="10 0 0 0"
                            materialDesign:HintAssist.Hint="Chọn loại nguồn ngân sách"
                            materialDesign:TextFieldAssist.HasClearButton="False"
                            IsEditable="False"
                            Style="{StaticResource MaterialDesignFloatingHintComboBox}"
                            DisplayMemberPath="DisplayItem"
                            SelectedValuePath="ValueItem"  
                            SelectedItem="{Binding BudgetSourceTypeSelected, UpdateSourceTrigger=PropertyChanged}"
                            ItemsSource="{Binding BudgetSourceTypes}"/>
                    </Grid>
                    <StackPanel Grid.Row="1"  Orientation="Horizontal" Margin="0 10 0 0">
                        <Button 
                        Margin="0 0 0 0"
                        Style="{StaticResource MaterialDesignRaisedButton}"
                        Command="{Binding GetFileFtpCommandHTTP}">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="Download" />
                                <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Lấy dữ liệu HTTP"/>
                            </StackPanel>
                        </Button>
                        <Button 
                        Margin="10 0 0 0"
                        Style="{StaticResource MaterialDesignRaisedButton}"
                        Command="{Binding GetFileFtpCommandFTP}">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="Download" />
                                <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Lấy dữ liệu FTP"/>
                            </StackPanel>
                        </Button>
                        <Button 
                        Margin="10 0 0 0"
                        Width="120"
                        Style="{StaticResource MaterialDesignRaisedButton}"
                        Command="{Binding ProcessFileCommand}">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="FileCheck" />
                                <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Kiểm tra"/>
                            </StackPanel>
                        </Button>
                        <Button 
                            Margin="10 0 0 0"
                            Width="120"
                            Click="BtnHuyBo_OnClick"
                            Style="{StaticResource MaterialDesignOutlinedButton}"
                            Command="{Binding ResetDataCommand}">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="Refresh" />
                                <TextBlock Style="{StaticResource SearchBarBtnTextStyle}" Text="Hủy bỏ"/>
                            </StackPanel>
                        </Button>
                    </StackPanel>
                    <Grid Grid.Row="2"
                    Margin="0 10 0 0">
                        <StackPanel Orientation="Horizontal">
                            <RadioButton
                                x:Name="RbSettlementVoucherDetail"
                                Style="{StaticResource MaterialDesignTabRadioButton}"
                                Visibility="{Binding VisibleNganSachSuDung}"
                                IsChecked="True"
                                Content="Chứng từ chi tiết ngân sách sử dụng" />
                            <RadioButton
                                x:Name="RbCanCu"
                                Style="{StaticResource MaterialDesignTabRadioButton}"
                                Visibility="{Binding VisibleNganSachDamBao}"
                                IsChecked="False"
                                Content="Chứng từ chi tiết ngân sách đặc thù của ngành" />
                            <RadioButton
                                x:Name="RbPhanCap"
                                Style="{StaticResource MaterialDesignTabRadioButton}"
                                IsChecked="False"
                                Visibility="{Binding VisibleNganSachDamBao}"
                                Content="Chứng từ phân cấp" />
                            <RadioButton
                                x:Name="RbError"
                                Style="{StaticResource MaterialDesignTabRadioButton}"
                                IsChecked="False"
                                Content="Lỗi" />
                        </StackPanel>
                    </Grid>
                </Grid>
                <Grid Grid.Column="1" Grid.RowSpan="2" Margin="5 5 0 0">
                    <!--Poppup Ftp-->
                    <DataGrid
                    x:Name="dgdDataPlanManagerIndex"
                    Grid.Row="2"
                    Margin="10 5 0 0"
                    FrozenColumnCount="0"
                    BorderThickness="1"
                    CanUserResizeColumns="True"
                    HorizontalScrollBarVisibility="Auto"
                    CanUserAddRows="False"
                    IsSynchronizedWithCurrentItem="True"
                    GridLinesVisibility="All"
                    MaxHeight="250"
                    AutoGenerateColumns="False"
                    RowStyle="{StaticResource DataGridRowIndex}"
                    Style="{StaticResource MaterialDesignDataGrid}"
                    ScrollViewer.CanContentScroll="True"
                    ItemsSource="{Binding LstFile, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                    SelectedItem="{Binding FileSelected, Mode=TwoWay}">
                        <DataGrid.Resources>
                            <helper:BindingProxy x:Key="DataContextProxy" Data="{Binding}" />
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <!--<DataGridCheckBoxColumn
                                        Binding="{Binding BIsCheck, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        ElementStyle="{StaticResource MaterialDesignDataGridCheckBoxColumnStyle}"
                                        EditingElementStyle="{StaticResource MaterialDesignDataGridCheckBoxColumnEditingStyle}">
                            </DataGridCheckBoxColumn>-->
                            <!--<materialDesign:DataGridTextColumn 
                                x:Name="STT"
                                ElementStyle="{StaticResource DataGridTextColumnStyle}" 
                                Header="STT"
                                IsReadOnly="True"
                                Binding="{Binding IStt}">
                            </materialDesign:DataGridTextColumn>-->
                            <materialDesign:DataGridTextColumn
                                x:Name="tenfile"
                                ElementStyle="{StaticResource DataGridTextColumnStyleImport}"
                                Header="Tên file"
                                IsReadOnly="True"
                                Binding="{Binding FileName}">
                            </materialDesign:DataGridTextColumn>
                            <materialDesign:DataGridTextColumn
                                x:Name="agencyName"
                                ElementStyle="{StaticResource DataGridTextColumnStyleImport}"
                                Header="Đơn vị"
                                IsReadOnly="True"
                                Binding="{Binding AgencyName}"
                                Width="200">
                            </materialDesign:DataGridTextColumn>
                            <materialDesign:DataGridTextColumn
                                x:Name="ngayGui"
                                ElementStyle="{StaticResource DataGridTextColumnStyleImport}"
                                Header="Ngày gửi"
                                IsReadOnly="True"
                                Width="*"
                                Binding="{Binding LastModifiedFormat}">
                            </materialDesign:DataGridTextColumn>
                            <DataGridTemplateColumn
                                Header="Tải xuống"
                                HeaderStyle="{StaticResource DataGridColumnHeaderCenter}"
                                Width="100">
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <StackPanel
                                            HorizontalAlignment="Center"
                                            Orientation="Horizontal">
                                            <Button
                                                ToolTip="Tải xuống"
                                                Style="{StaticResource MaterialDesignToolButton}"
                                                Command="{Binding DataContext.DownloadFileFtpServer, ElementName=dgdDataPlanManagerIndex}"
                                                CommandParameter="{Binding SelectedItem, ElementName=dgdDataPlanManagerIndex}">
                                                <materialDesign:PackIcon
                                                    Kind="CloudDownloadOutline" />
                                            </Button>
                                        </StackPanel>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    <!--End Ftp-->
                </Grid>
                <Grid Grid.ColumnSpan="2"
                    Grid.Row="2"
                    Margin="0 5 0 0">

                    <!--NS_DTDauNam_ChungTuChiTiet-->
                    <ScrollViewer 
                        HorizontalScrollBarVisibility="Auto"
                        VerticalScrollBarVisibility="Auto"
                        Visibility="{Binding IsChecked, ElementName=RbSettlementVoucherDetail, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <DataGrid
                            x:Name="dgChungTuChiTietImport"
                            AutoGenerateColumns="False" 
                            CanUserResizeColumns="True"
                            BorderThickness="1"
                            FrozenColumnCount ="2"
                            CanUserAddRows="False"
                            GridLinesVisibility="All"
                            Style="{StaticResource MaterialDesignDataGrid}"
                            CellStyle="{StaticResource GenericDataGridCellDetail}"
                            RowStyle="{StaticResource GenericDataGridRowDetail}"
                            SelectedItem="{Binding SelectedItem}"
                            ItemsSource="{Binding DataBeginYearImport}">
                            <DataGrid.Resources>
                                <helper:BindingProxy
                            x:Key="DataContextProxy"
                            Data="{Binding}" />
                            </DataGrid.Resources>
                            <DataGrid.Columns>
                                <DataGridTemplateColumn Header="Trạng thái">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <ContentControl x:Name="content"/>
                                            <DataTemplate.Triggers>
                                                <DataTrigger Binding="{Binding ImportStatus}" Value="True">
                                                    <Setter TargetName="content" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <materialDesign:PackIcon Kind="CheckCircle" Foreground="Green" HorizontalAlignment="Center"/>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding ImportStatus}" Value="False">
                                                    <Setter TargetName="content" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <materialDesign:PackIcon Kind="CloseCircle" Foreground="Red" HorizontalAlignment="Center"/>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding IsWarning}" Value="True">
                                                    <Setter TargetName="content" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <materialDesign:PackIcon Kind="WarningCircleOutline" Foreground="Orange" HorizontalAlignment="Center"/>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                            </DataTemplate.Triggers>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTextColumn Header="LNS" Binding="{Binding LNS}"/>
                                <DataGridTextColumn Header="L" Binding="{Binding L}"/>
                                <DataGridTextColumn Header="K" Binding="{Binding K}"/>
                                <DataGridTextColumn Header="M" Binding="{Binding M}"/>
                                <DataGridTextColumn Header="TM" Binding="{Binding TM}"/>
                                <DataGridTextColumn Header="TTM" Binding="{Binding TTM}"/>
                                <DataGridTextColumn Header="NG" Binding="{Binding NG}"/>
                                <DataGridTextColumn Header="TNG" Binding="{Binding TNG}"/>
                                <DataGridTextColumn Header="TNG1" Binding="{Binding TNG1}"/>
                                <DataGridTextColumn Header="TNG2" Binding="{Binding TNG2}"/>
                                <DataGridTextColumn Header="TNG3" Binding="{Binding TNG3}"/>
                                <DataGridTextColumn Header="Mô tả" Binding="{Binding MoTa}" IsReadOnly="True"/>
                                <!--<DataGridTextColumn 
                            Header="Dự toán"
                            IsReadOnly="True"
                            Binding="{Binding DuToan, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                            <DataGridTextColumn 
                            Header="Quyết toán"
                            IsReadOnly="True"
                            Binding="{Binding QuyetToan, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>-->
                                <DataGridTextColumn 
                            Header="{Binding Data.STuChi1, Source={StaticResource DataContextProxy}}"
                            IsReadOnly="True"
                            Binding="{Binding FTuChi1, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn 
                            Header="{Binding Data.STuChi2, Source={StaticResource DataContextProxy}}"
                            IsReadOnly="True"
                            Binding="{Binding FTuChi2, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn 
                            Header="{Binding Data.STuChi3, Source={StaticResource DataContextProxy}}"
                            IsReadOnly="True"
                            Binding="{Binding FTuChi3, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn 
                            Header="{Binding Data.STuChi4, Source={StaticResource DataContextProxy}}"
                            IsReadOnly="True"
                            Binding="{Binding FTuChi4, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn 
                            Header="{Binding Data.STuChi5, Source={StaticResource DataContextProxy}}"
                            IsReadOnly="True"
                            Binding="{Binding FTuChi5, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>

                                <DataGridTextColumn 
                            Header="Ước T.Hiện"
                            IsReadOnly="True"
                            Binding="{Binding UocThucHien, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn 
                            Header="Chi tiết"
                            IsReadOnly="True"
                            Visibility="{Binding Data.VisibleNganSachSuDung, Source={StaticResource DataContextProxy}}"
                            Binding="{Binding ChiTiet, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn
                            Visibility="{Binding Data.VisibleNganSachDamBao, Source={StaticResource DataContextProxy}}"
                            Header="Hàng nhập"
                            IsReadOnly="True"
                            Binding="{Binding HangNhap, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn
                            Visibility="{Binding Data.VisibleNganSachDamBao, Source={StaticResource DataContextProxy}}"
                            Header="Hàng mua"
                            IsReadOnly="True"
                            Binding="{Binding HangMua, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn
                            Visibility="{Binding Data.VisibleNganSachDamBao, Source={StaticResource DataContextProxy}}"
                            Header="Phân cấp"
                            IsReadOnly="True"
                            Binding="{Binding PhanCap, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTextColumn
                            Visibility="{Binding Data.VisibleNganSachDamBao, Source={StaticResource DataContextProxy}}"
                            Header="Chưa phân cấp"
                            IsReadOnly="True"
                            Binding="{Binding ChuaPhanCap, StringFormat={}{0:N0}}"
                            ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"/>
                                <DataGridTemplateColumn Header="Lỗi">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <ContentControl x:Name="Function"/>
                                            <DataTemplate.Triggers>
                                                <DataTrigger Binding="{Binding ImportStatus}" Value="False">
                                                    <Setter TargetName="Function" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <Button
                                                            Style="{StaticResource MaterialDesignIconButton}"
                                                            ToolTip="Xem thông tin lỗi"
                                                            Width="25"
                                                            Height="25"
                                                            Command="{Binding Path=DataContext.ShowErrorCommand, RelativeSource= {RelativeSource FindAncestor, AncestorType={x:Type DataGrid}}}">
                                                                    <materialDesign:PackIcon
                                                                Foreground="DarkRed"
                                                                Width="15"
                                                                Height="15"
                                                                Kind="EyeRemove"/>
                                                                </Button>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding IsWarning}" Value="True">
                                                    <Setter TargetName="Function" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                            </DataTemplate.Triggers>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTemplateColumn Header="MLNS">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <ContentControl x:Name="Function"/>
                                            <DataTemplate.Triggers>
                                                <DataTrigger Binding="{Binding IsErrorMLNS}" Value="True">
                                                    <Setter TargetName="Function" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <Button
                                                                    Style="{StaticResource MaterialDesignIconButton}"
                                                                    ToolTip="Thêm mục lục ngân sách"
                                                                    Width="25"
                                                                    Height="25"
                                                                    Click="ButtonOpenError_Click">
                                                                    <materialDesign:PackIcon
                                                                Foreground="Green"
                                                                Width="15"
                                                                Height="15"
                                                                Kind="FileMoveOutline"/>
                                                                </Button>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding IsWarning}" Value="True">
                                                    <Setter TargetName="Function" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                            </DataTemplate.Triggers>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                    </ScrollViewer>

                    <!--NS_DTDauNam_ChungTuChiTiet_CanCu-->
                    <ScrollViewer 
                        x:Name="dgChungTuCanCu"
                        HorizontalScrollBarVisibility="Auto"
                        VerticalScrollBarVisibility="Auto"
                        Visibility="{Binding IsChecked, ElementName=RbCanCu, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <DataGrid
                            AutoGenerateColumns="False" 
                            CanUserResizeColumns="True"
                            BorderThickness="1"
                            FrozenColumnCount ="2"
                            CanUserAddRows="False"
                            GridLinesVisibility="All"
                            Style="{StaticResource MaterialDesignDataGrid}"
                            CellStyle="{StaticResource GenericDataGridCellDetail}"
                            RowStyle="{StaticResource GenericDataGridRowDetail}"
                            SelectedItem="{Binding CanCuSelected}"
                            ItemsSource="{Binding CanCuItems}">
                            <DataGrid.Resources>
                                <helper:BindingProxy
                            x:Key="DataContextProxy"
                            Data="{Binding}" />
                            </DataGrid.Resources>
                            <DataGrid.Columns>
                                <DataGridTemplateColumn Header="Trạng thái">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <ContentControl x:Name="content"/>
                                            <DataTemplate.Triggers>
                                                <DataTrigger Binding="{Binding ImportStatus}" Value="True">
                                                    <Setter TargetName="content" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <materialDesign:PackIcon Kind="CheckCircle" Foreground="Green" HorizontalAlignment="Center"/>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding ImportStatus}" Value="False">
                                                    <Setter TargetName="content" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <materialDesign:PackIcon Kind="CloseCircle" Foreground="Red" HorizontalAlignment="Center"/>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding IsWarning}" Value="True">
                                                    <Setter TargetName="content" Property="ContentTemplate">
                                                        <Setter.Value>
                                                            <DataTemplate>
                                                                <materialDesign:PackIcon Kind="WarningCircleOutline" Foreground="Orange" HorizontalAlignment="Center"/>
                                                            </DataTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                </DataTrigger>
                                            </DataTemplate.Triggers>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                                <DataGridTextColumn Header="LNS" Binding="{Binding LNS}"/>
                                <DataGridTextColumn Header="L" Binding="{Binding L}"/>
                                <DataGridTextColumn Header="K" Binding="{Binding K}"/>
                                <DataGridTextColumn Header="M" Binding="{Binding M}"/>
                                <DataGridTextColumn Header="TM" Binding="{Binding TM}"/>
                                <DataGridTextColumn Header="TTM" Binding="{Binding TTM}"/>
                                <DataGridTextColumn Header="NG" Binding="{Binding NG}"/>
                                <DataGridTextColumn Header="TNG" Binding="{Binding TNG}"/>
                                <DataGridTextColumn Header="TNG1" Binding="{Binding TNG1}"/>
                                <DataGridTextColumn Header="TNG2" Binding="{Binding TNG2}"/>
                                <DataGridTextColumn Header="TNG3" Binding="{Binding TNG3}"/>
                                <DataGridTextColumn Header="Mô tả" Binding="{Binding Description}" IsReadOnly="True"/>
                                <component:DataGridTextColumn
                                x:Name="HN1"
                                Header="Hàng nhập"
                                StringFormat="{}{0:N0}"
                                HeaderStyle="{StaticResource DataGridColumnHeaderNoBorder}"
                                IsReadOnly="True"
                                Width="100"
                                Binding="{Binding SHangNhap1, StringFormat={}{0:N0}}"
                                ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}">
                                    <DataGridColumn.HeaderTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition />
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Border
                                                Padding="{Binding RelativeSource={RelativeSource Self}, Path=(wpf:DataGridAssist.ColumnHeaderPadding), Converter={StaticResource HeaderPaddingConverter}}">
                                                    <Canvas Height="{Binding ElementName=HeaderGroup2, Path=ActualHeight}">
                                                        <Grid>
                                                            <Grid.Style>
                                                                <Style TargetType="Grid">
                                                                    <Setter Property="Width">
                                                                        <Setter.Value>
                                                                            <MultiBinding Converter="{StaticResource MultipleWidthConverter}">
                                                                                <Binding ElementName="HN1" Path="ActualWidth"/>
                                                                                <Binding ElementName="HM1" Path="ActualWidth"></Binding>
                                                                            </MultiBinding>
                                                                        </Setter.Value>
                                                                    </Setter>
                                                                </Style>
                                                            </Grid.Style>
