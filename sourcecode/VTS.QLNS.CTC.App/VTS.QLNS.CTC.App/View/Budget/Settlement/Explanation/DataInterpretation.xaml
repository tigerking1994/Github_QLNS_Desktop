                                    <Button Style="{StaticResource SearchBarBtnStyle}"
                                            ToolTip="Refresh"
                                            Command="{Binding ReloadDataCommand}">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="Refresh" />
                                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}"
                                                       Text="(F5)" />
                                        </StackPanel>
                                    </Button>
                                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                                    <Button Style="{StaticResource SearchBarBtnStyle}"
                                            Visibility="{Binding IsEnableSelfPayTab, Converter={StaticResource BooleanToVisibilityConverter}}"
                                            IsEnabled="{Binding IsDeleteAll}"
                                            Command="{Binding DeleteAllCommand}">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="DeleteForever" />
                                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}"
                                                       Text="Xóa tất cả" />
                                        </StackPanel>
                                    </Button>
                                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}"
                                               Visibility="{Binding IsEnableSelfPayTab, Converter={StaticResource BooleanToVisibilityConverter}}" />
                                    <Button Style="{StaticResource SearchBarBtnStyle}"
                                            IsEnabled="{Binding IsSaveData}"
                                            Visibility="{Binding IsEnableSelfPayTab, Converter={StaticResource BooleanToVisibilityConverter}}"
                                            Command="{Binding SaveCommand}">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="ContentSave" />
                                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}"
                                                       Text="Lưu số liệu" />
                                        </StackPanel>
                                    </Button>
                                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}"
                                               Visibility="{Binding IsEnableSelfPayTab, Converter={StaticResource BooleanToVisibilityConverter}}" />
                                    <Button Style="{StaticResource SearchBarBtnStyle}"
                                            Name="btnSaveAndUpdate"
                                            IsEnabled="{Binding IsSaveAndUpdateData}"
                                            Visibility="{Binding IsEnableSelfPayTab, Converter={StaticResource BooleanToVisibilityConverter}}"
                                            Command="{Binding SaveAndUpdateCommand}">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="ContentSave" />
                                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}"
                                                       Text="Lưu và cập nhật" />
                                        </StackPanel>
                                    </Button>
                                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}"
                                               Visibility="{Binding IsEnableSelfPayTab, Converter={StaticResource BooleanToVisibilityConverter}}" />
                                    <Button x:Name="btnShowColSelfPay"
                                            Style="{StaticResource SearchBarBtnStyle}">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Kind="PlusMinusVariant" />
                                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}"
                                                       Text="Ẩn/hiện cột" />
                                            <materialDesign:PopupBox Name="pbDynamicColumn"
                                                                     Margin="-10 0 0 0"
                                                                     HorizontalAlignment="Right"
                                                                     StaysOpen="True">
                                                <materialDesign:PopupBox.ToggleContent>
                                                    <materialDesign:PackIcon Kind="MenuDown" />
                                                </materialDesign:PopupBox.ToggleContent>
                                            </materialDesign:PopupBox>
                                        </StackPanel>
                                    </Button>
                                    <Separator Style="{StaticResource SearchBarHorizontalDividerStyle}" />
                                    <ComboBox Margin="0 0 0 5"
                                              Name="cbFrozenColumn"
                                              materialDesign:HintAssist.Hint="Cố định cột"
                                              DisplayMemberPath="DisplayItem"
                                              SelectedValuePath="ValueItem" />
                                </StackPanel>
                            </Border>
                            <component:ExpandedDataGrid ColumnInfo="{Binding Columns}"
                                                        ElementFrozenColumn="{Binding ElementName=cbFrozenColumn, Path=., Mode=TwoWay}"
                                                        ElementDynamicColumn="{Binding ElementName=pbDynamicColumn, Path=., Mode=TwoWay}"
                                                        Style="{StaticResource MaterialDesignDataGrid}"
                                                        x:Name="DgDataInterpretation"
                                                        IsEnabled="{Binding IsEnableSelfPayTab}"
                                                        Grid.Row="1"
                                                        AutoGenerateColumns="False"
                                                        CanUserResizeColumns="True"
                                                        BorderThickness="1"
                                                        VerticalAlignment="Stretch"
                                                        CanUserAddRows="False"
                                                        FrozenColumnCount="3"
                                                        GridLinesVisibility="All"
                                                        CellStyle="{StaticResource DataGridCellDetail}"
                                                        RowStyle="{StaticResource DataGridRowDetail}"
                                                        ItemsSource="{Binding ExplainSubtracts}"
                                                        SelectedItem="{Binding SelectedExplainSubtract}"
                                                        ScrollViewer.ScrollChanged="dgdData_ScrollChanged"
                                                        wpf:DataGridAssist.CornerRadius="0 0 4 4">
                                <DataGrid.Columns>
                                    <DataGridTemplateColumn x:Name="IdDoiTuong"
                                                            Header="Đối tượng">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <DataTemplate.Resources>
                                                    <Style TargetType="ComboBox"
                                                           BasedOn="{StaticResource {ComponentResourceKey TypeInTargetAssembly={x:Type ComboBox}, ResourceId=MaterialDataGridComboBoxColumnEditingStyle}}">
                                                    </Style>
                                                </DataTemplate.Resources>
                                                <ComboBox DisplayMemberPath="Name"
                                                          SelectedValuePath="Code"
                                                          SelectedValue="{Binding IIdDoiTuong, UpdateSourceTrigger=LostFocus}"
                                                          ItemsSource="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}, Path=DataContext.MilitaryObjects}">
                                                </ComboBox>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTextColumn x:Name="IdDonVi"
                                                        Header="Đơn vị"
                                                        Binding="{Binding IIdMaDonVi, UpdateSourceTrigger=LostFocus}"
                                                        IsReadOnly="True"
                                                        ElementStyle="{StaticResource DataGridTextColumnDetailStyle}" />
                                    <DataGridTextColumn x:Name="HoTen"
                                                        Header="Họ tên"
                                                        Binding="{Binding SHoTen, UpdateSourceTrigger=LostFocus}"
                                                        ElementStyle="{StaticResource DataGridTextColumnDetailStyle}"
                                                        EditingElementStyle="{StaticResource DataGridTextColumnEditingStyle}" />
                                    <DataGridTextColumn x:Name="LuongThang"
                                                        Header="Tổng cộng"
                                                        Binding="{Binding FLuongThang, UpdateSourceTrigger=PropertyChanged, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                        ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                        EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}"
                                                        IsReadOnly="True" />
                                    <component:DataGridTextColumn x:Name="NgayNghi"
                                                                  Header="Ngày nghỉ"
                                                                  StringFormat="{}{0:N0}"
                                                                  Binding="{Binding FNgayNghi, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                                  ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                                  EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}" />
                                    <component:DataGridTextColumn x:Name="SoNguoi"
                                                                  Header="Người"
                                                                  StringFormat="{}{0:N0}"
                                                                  Binding="{Binding FSoNguoi, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                                  ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                                  EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}" />
                                    <component:DataGridTextColumn x:Name="LuongCapBac"
                                                                  Header="Lương CB"
                                                                  StringFormat="{}{0:N0}"
                                                                  Binding="{Binding FLuongCapBac, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                                  ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                                  EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}" />
                                    <component:DataGridTextColumn x:Name="LuongThamNien"
                                                                  Header="Thâm niên"
                                                                  StringFormat="{}{0:N0}"
                                                                  Binding="{Binding FLuongThamNien, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                                  ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                                  EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}" />
                                    <component:DataGridTextColumn x:Name="LuongPhuCapCongVu"
                                                                  Header="Phụ cấp CV"
                                                                  StringFormat="{}{0:N0}"
                                                                  Binding="{Binding FLuongPhuCapCongVu, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                                  ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                                  EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}" />
                                    <component:DataGridTextColumn x:Name="LuongPhuCapKhacBh"
                                                                  Header="PC khác BH"
                                                                  StringFormat="{}{0:N0}"
                                                                  Binding="{Binding FLuongPhuCapKhacBh, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                                  ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                                  EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}" />
                                    <component:DataGridTextColumn x:Name="LuongPhuCapKhac"
                                                                  Header="PC khác"
                                                                  StringFormat="{}{0:N0}"
                                                                  Binding="{Binding FLuongPhuCapKhac, UpdateSourceTrigger=LostFocus, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                                  ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                                  EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}" />
                                    <DataGridTextColumn x:Name="TongBaoHiem"
                                                        Header="Tính BH"
                                                        Binding="{Binding FTongBaoHiem, UpdateSourceTrigger=PropertyChanged, StringFormat={}{0:N0}, Converter={StaticResource NumberToStringEmptyConverter}}"
                                                        ElementStyle="{StaticResource DataGridTextColumnDetailRightStyle}"
                                                        EditingElementStyle="{StaticResource DataGridTextColumnEditingRightCell}"
                                                        IsReadOnly="True" />
                                </DataGrid.Columns>
                            </component:ExpandedDataGrid>
                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0"
                                            Orientation="Horizontal">
                                    <Grid Width="{Binding ElementName=IdDoiTuong, Path=ActualWidth}"></Grid>
                                    <Grid Width="{Binding ElementName=IdDonVi, Path=ActualWidth}"></Grid>
                                    <Grid Width="{Binding ElementName=HoTen, Path=ActualWidth}"></Grid>
                                </StackPanel>
                                <ScrollViewer Grid.Column="1"
                                              x:Name="scrollFooter"
                                              HorizontalScrollBarVisibility="Hidden"
                                              VerticalScrollBarVisibility="Hidden">
                                    <StackPanel Orientation="Horizontal">
                                        <Grid Width="{Binding ElementName=LuongThang, Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongLuongThang, StringFormat={}{0:N0}}"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=NgayNghi, Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongNgayNghi, StringFormat={}{0:N0}}"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=SoNguoi, Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongSoNguoi, StringFormat={}{0:N0}}"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=LuongCapBac,Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongLuongCapBac, StringFormat={}{0:N0}}"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=LuongThamNien,Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongLuongThamNien, StringFormat={}{0:N0}}"
                                                     IsReadOnly="True"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=LuongPhuCapCongVu,Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongLuongPhuCapCongVu, StringFormat={}{0:N0}}"
                                                     IsReadOnly="True"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=LuongPhuCapKhacBh,Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongLuongPhuCapKhacBh, StringFormat={}{0:N0}}"
                                                     IsReadOnly="True"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=LuongPhuCapKhac,Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongLuongPhuCapKhac, StringFormat={}{0:N0}}"
                                                     IsReadOnly="True"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                        <Grid Width="{Binding ElementName=TongBaoHiem,Path=ActualWidth}">
                                            <TextBox Text="{Binding SumExplainSubtract.TongBaoHiem, StringFormat={}{0:N0}}"
                                                     IsReadOnly="True"
                                                     Style="{StaticResource TextBoxTotalCell}"></TextBox>
                                        </Grid>
                                    </StackPanel>
                                </ScrollViewer>
                            </Grid>
                        </Grid>
                    </TabItem>
                </TabControl>
                <StackPanel Grid.Row="3"
                            Orientation="Horizontal"
                            Margin="0 10 0 0"
                            HorizontalAlignment="Right">
                    <Button Margin="5 0 5 0"
                            Style="{StaticResource MaterialDesignRaisedButton}"
                            Command="{Binding SaveCommand}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="ContentSave" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}"
                                       Text="Lưu dữ liệu" />
                        </StackPanel>
                    </Button>
                    <Button IsCancel="True"
                            Margin="5 0 0 0"
                            Style="{StaticResource MaterialDesignOutlinedButton}"
                            Command="{Binding CloseCommand}"
                            CommandParameter="{Binding ElementName=DataInterpretationWindow}">
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="CloseOutline" />
                            <TextBlock Style="{StaticResource SearchBarBtnTextStyle}"
                                       Text="Đóng" />
                        </StackPanel>
                    </Button>
                </StackPanel>
            </Grid>
        </ScrollViewer>

    </materialDesign:DialogHost>
</Window>
